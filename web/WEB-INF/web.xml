<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>
	Fabrica4</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	
	<servlet>
	   <servlet-name>CadastraUsuario</servlet-name>
	   <servlet-class>br.usp.poli.pece.servlet.CadastraUsuarioServlet</servlet-class>
	</servlet>
	
    <servlet>
       <servlet-name>MatriculaAluno</servlet-name>
       <servlet-class>br.usp.poli.pece.servlet.MatriculaAlunoServlet</servlet-class>
    </servlet>
    
    <servlet>
       <servlet-name>ListaUsuarios</servlet-name>
       <servlet-class>br.usp.poli.pece.servlet.ListaUsuariosServlet</servlet-class>
    </servlet>
    
    <servlet>
       <servlet-name>CadastraCurso</servlet-name>
       <servlet-class>br.usp.poli.pece.servlet.CadastraCursoServlet</servlet-class>
    </servlet>
	
	<servlet-mapping>
	   <servlet-name>CadastraUsuario</servlet-name>
	   <url-pattern>/academico/CadastraUsuario</url-pattern>
	</servlet-mapping>
    
    <servlet-mapping>
       <servlet-name>ListaUsuarios</servlet-name>
       <url-pattern>/academico/ListaUsuarios</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
       <servlet-name>MatriculaAluno</servlet-name>
       <url-pattern>/academico/MatriculaAluno</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
       <servlet-name>CadastraCurso</servlet-name>
       <url-pattern>/academico/CadastraCurso</url-pattern>
    </servlet-mapping>
    
	
</web-app>
